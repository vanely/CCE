{
  "manifest_version": 3,
  "name": "Claude Code Extractor",
  "version": "1.0.0",
  "description": "Automatically extract and organize Claude code artifacts into your project directory",
  "permissions": [
    "activeTab",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "*://claude.ai/*",
    "http://localhost:*"
  ],
  "content_scripts": [{
    "matches": ["*://claude.ai/*"],
    "js": ["content-script.js"],
    "run_at": "document_end"
  }],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Claude Code Extractor"
  },
  "icons": {
